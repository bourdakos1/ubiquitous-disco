{
    "extends": [
        "eslint:recommended"
    ],
    "env": {
        "browser": true,
        "es6": true
    },
    "rules": {
        "semi": "error",
        "eqeqeq": "error",
        "prefer-const": "warn",
        "no-console": "warn",
        "no-unused-vars": ["error", { "vars": "local", "args": "none" }]
    }
}
